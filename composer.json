{
    "name": "nenad/yii2-advanced-template",
    "description": "Improved Yii 2 Advanced Application Template By Nenad Zivkovic",
    "keywords": ["yii2", "framework", "advanced", "improved", "application template", "nenad"],
    "type": "project",
    "license": "BSD-3-Clause",
    "support": {
        "tutorial": "http://www.freetuts.org/tutorial/view?id=6",
        "source": "https://github.com/nenad-zivkovic/yii2-advanced-template"
    },
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "*",
        "yiisoft/yii2-bootstrap": "*",
        "yiisoft/yii2-swiftmailer": "*",
        "nenad/yii2-password-strength": "*",
        "mihaildev/yii2-ckeditor": "*",
        "kartik-v/yii2-widget-fileinput": "@dev",
        "nemmo/yii2-attachments": "dev-master",
        "mootensai/yii2-enhanced-gii": "dev-master",
        "kartik-v/yii2-checkbox-x": "@dev",
        "beastbytes/yii2-wizard": "^1.0",
        "nirvana-msu/yii2-showloading": "1.0.*@dev",
        "kartik-v/yii2-widget-growl": "*",
        "kartik-v/yii2-widget-select2": "@dev",
        "2amigos/yii2-gallery-widget": "~1.0",
        "kartik-v/yii2-detail-view": "@dev",
        "metalguardian/yii2-fotorama-widget": "*",
        "jlorente/yii2-widget-remainingcharacters": "*",
        "raoul2000/yii2-scrollup-widget": "*",
        "evgeniyrru/yii2-slick": "*",
        "kartik-v/yii2-icons": "@dev",
        "yiisoft/yii2-imagine": "^2.0"

    },
    "require-dev": {
        "yiisoft/yii2-codeception": "*",
        "yiisoft/yii2-debug": "*",
        "yiisoft/yii2-gii": "*",
        "yiisoft/yii2-faker": "*",
        "codeception/specify": "*",
        "codeception/verify": "*"
    },
    "config": {
        "vendor-dir": "_protected/vendor",
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "_protected/vendor/npm",
            "bower-asset-library": "_protected/vendor/bower"
        }
    }
}
